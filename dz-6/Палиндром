#include <iostream>
#include <string.h>
#include <windows.h>
#include <cstdlib>
using namespace std;

int main()
{
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);
	setlocale(LC_ALL, "Russian");

	char str[80];
	cin.get(str, (unsigned)_countof(str));
	bool pal = true;

	for (int i = 0; i < (strlen(str) / 2); i++)
		if (str[i] != str[strlen(str) - (i + 1)]) pal = false;
	if (pal == true) cout << "Палиндром\n";
	else cout << "Не палиндром\n";

	system("pause");
	return 0;
}
