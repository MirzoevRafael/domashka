#include <iostream>
using namespace std;

int fibonacci(int n)
{
	if ((n == 1) || (n == 2))
	{
		return 1;
	}

	else
	{
		return fibonacci(n - 1) + fibonacci(n - 2);
	}
}

int main() {
	setlocale(LC_ALL, "Russian");

	int num;
	cin >> num;

	cout << fibonacci(num) << endl;

	system("pause");
	return 0;
}
